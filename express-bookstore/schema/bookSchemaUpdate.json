{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "minProperties": 1,
    "properties": {
        "isbn": {
            "type": "string",
            "default": "",
            "title": "The isbn Schema",
            "examples": ["978-0-123456-78-9"],
            "readOnly": true
        },
        "amazon_url": {
            "type": "string",
            "default": "",
            "title": "The amazon_url Schema",
            "examples": ["https://www.amazon.com/example-book"]
        },
        "author": {
            "type": "string",
            "default": "",
            "title": "The author Schema",
            "examples": ["John Author"]
        },
        "language": {
            "type": "string",
            "default": "",
            "title": "The language Schema",
            "examples": ["English"]
        },
        "pages": {
            "type": "integer",
            "default": 0,
            "title": "The pages Schema",
            "examples": [300]
        },
        "publisher": {
            "type": "string",
            "default": "",
            "title": "The publisher Schema",
            "examples": ["Publisher XYZ"]
        },
        "title": {
            "type": "string",
            "default": "",
            "title": "The title Schema",
            "examples": ["The Example Book"]
        },
        "year": {
            "type": "integer",
            "default": 0,
            "title": "The year Schema",
            "examples": [2023]
        }
    },
    "examples": [
        {
            "isbn": "978-0-123456-78-9",
            "amazon_url": "https://www.amazon.com/example-book",
            "author": "John Author",
            "language": "English",
            "pages": 300,
            "publisher": "Publisher XYZ",
            "title": "The Example Book",
            "year": 2023
        }
    ]
}
